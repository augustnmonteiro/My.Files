#Function to resource
function resource(){
    echo "Source $HOME/.dotfiles/.zshrc"
    source $HOME/.dotfiles/.zshrc

    if [[ $? == 0 ]]; then
      echo "Source Ok!"
    else
      echo "Some error on source!"
    fi
}
